<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?language javascript?>

<GridPane alignment="center" hgap="10" styleClass="root" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="naivgationdrawer.controller.FXMLAjouterCategorieDialogController">
    
    
    
    
    <padding><Insets bottom="10" left="25" right="25" top="25" /></padding>
   <children>
     
     <Text id="welcome-text" text="Ajouter categorie" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="0" />
     
     
      <Label text="categorie_parent_id" GridPane.columnIndex="0" GridPane.rowIndex="2" />
     
     <ComboBox fx:id="categorie_parent_id" prefHeight="25.0" prefWidth="183.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
    
      
     <Label text="nom" GridPane.columnIndex="0" GridPane.rowIndex="3" />
     
     <TextField fx:id="nom" GridPane.columnIndex="1" GridPane.rowIndex="3" />
     
     <HBox alignment="bottom_right" spacing="10" GridPane.columnIndex="1" GridPane.rowIndex="4">
        <children>
            <Button fx:id="confirmer" onAction="#handleSubmitButtonAction" text="confirmer" />
            <Button fx:id="cancel" onAction="#handleButtonCancelar" text="Cancel" />
        </children>
     </HBox>
     
     <Text fx:id="actiontarget" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.halignment="RIGHT" GridPane.rowIndex="6" />
   </children>
  
  <stylesheets>
    <URL value="@Login.css" />
  </stylesheets>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
 
</GridPane>
